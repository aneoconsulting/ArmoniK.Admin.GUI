    "@angular/platform-browser": "~14.2.12",
    "@angular/platform-browser-dynamic": "~14.2.12",
    "@angular/router": "~14.2.12",
    "@cds/core": "^6.2.2",
    "@clr/angular": "^13.10.1",
    "@clr/icons": "^13.0.2",
    "@clr/ui": "^13.8.2",
    "@grpc/grpc-js": "^1.6.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.4.5",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@ngx-grpc/common": "^3.0.3",
    "@ngx-grpc/core": "^3.0.3",
    "@ngx-grpc/grpc-web-client": "^3.0.3",
