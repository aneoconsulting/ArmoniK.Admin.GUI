{
  "name": "armonik.admin.gui",
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx run-many --target=serve --all",
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "e2e": "nx run-many --target=e2e --all",
    "e2e:watch": "nx run-many --target=e2e --all --watch",
    "format": "nx format --base=main",
    "format:check": "nx format:check --base=main",
    "lint": "nx run-many --target=lint --all",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "proto:generate": "protoc --plugin=protoc-gen-ng=$(which protoc-gen-ng) --ng_out=./apps/app/src/app/core/types/proto -I ./apps/app/src/assets/ArmoniK.Api/Protos/V1 ./apps/app/src/assets/ArmoniK.Api/Protos/V1/*.proto",
    "proto:generate:win": "for /f %G in ('dir /b .\\apps\\app\\src\\assets\\ArmoniK.Api\\Protos\\V1\\*.proto') do grpc_tools_node_protoc --plugin=protoc-gen-ng=.\\node_modules\\.bin\\protoc-gen-ng.cmd --ng_out=.\\apps\\app\\src\\app\\core\\types\\proto -I .\\apps\\app\\src\\assets\\ArmoniK.Api\\Protos\\V1 .\\apps\\app\\src\\assets\\ArmoniK.Api\\Protos\\V1\\%G"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@clr/angular": "^13.10.1",
    "@clr/icons": "^13.0.2",
    "@clr/ui": "^13.10.1",
    "@grpc/grpc-js": "^1.7.3",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/swagger": "^6.1.3",
    "@ngx-grpc/common": "^3.0.3",
    "@ngx-grpc/core": "^3.0.3",
    "@ngx-grpc/grpc-web-client": "^3.0.3",
    "@ngx-grpc/well-known-types": "^3.0.3",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@nrwl/angular": "15.1.1",
    "@webcomponents/webcomponentsjs": "^2.7.0",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "mongoose": "^6.7.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.7",
    "swagger-ui-express": "^4.6.0",
    "tslib": "^2.4.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular/cli": "~14.2.9",
    "@angular/compiler-cli": "^14.2.0",
    "@angular-eslint/eslint-plugin": "~14.3.0",
    "@angular-eslint/eslint-plugin-template": "~14.3.0",
    "@angular-eslint/template-parser": "~14.3.0",
    "@angular/language-service": "~14.2.10",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@ngx-grpc/protoc-gen-ng": "^3.0.3",
    "@nrwl/cli": "15.1.1",
    "@nrwl/cypress": "15.1.1",
    "@nrwl/eslint-plugin-nx": "15.1.1",
    "@nrwl/jest": "15.1.1",
    "@nrwl/linter": "15.1.1",
    "@nrwl/nest": "15.1.1",
    "@nrwl/node": "15.1.1",
    "@nrwl/workspace": "15.1.1",
    "@types/google-protobuf": "^3.15.6",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "~5.43.0",
    "@typescript-eslint/parser": "~5.43.0",
    "cypress": "^11.1.0",
    "eslint": "~8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "grpc-tools": "^1.11.3",
    "husky": "^7.0.4",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "29.3.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^12.4.0",
    "mongodb-memory-server": "^8.10.0",
    "nx": "15.1.1",
    "prettier": "^2.7.1",
    "ts-jest": "29.0.3",
    "typescript": "~4.6.2"
  },
  "lint-staged": {
    "**": [
      "npm run format:check",
      "npm run lint",
      "npm run test"
    ]
  }
}
