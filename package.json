    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^13.1.2",
    "ng-extract-i18n-merge": "2.7.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "jest-junit": {
    "outputDirectory": "./coverage"
  }
