{
  "name": "armonik.admin.gui",
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && yarn proto:generate",
    "start": "yarn proto:generate && nx run-many --target=serve --all",
    "build": "yarn proto:generate && nx run-many --target=build --all",
    "test": "yarn proto:generate && nx run-many --target=test --all",
    "e2e": "yarn proto:generate && nx run-many --target=e2e --all",
    "e2e:watch": "yarn proto:generate && nx run-many --target=e2e --all --watch",
    "format": "nx format --base=main",
    "format:check": "nx format:check --base=main",
    "lint": "nx run-many --target=lint --all",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "proto:generate": "node ./scripts/generate-proto.mjs",
    "proto:generate:linux": "protoc --plugin=protoc-gen-ng=$(which protoc-gen-ng) --ng_out=./libs/shared/util/src/lib/proto/generated -I ./libs/shared/util/src/lib/proto/ArmoniK.Api/Protos/V1 ./libs/shared/util/src/lib/proto/ArmoniK.Api/Protos/V1/*.proto",
    "proto:generate:win": "for /f %G in ('dir /b .\\libs\\shared\\util\\src\\lib\\proto\\ArmoniK.Api\\Protos\\V1\\*.proto') do grpc_tools_node_protoc --plugin=protoc-gen-ng=.\\node_modules\\.bin\\protoc-gen-ng.cmd --ng_out=.\\libs\\shared\\util\\src\\lib\\proto\\generated -I .\\libs\\shared\\util\\src\\lib\\proto\\ArmoniK.Api\\Protos\\V1 .\\libs\\shared\\util\\src\\lib\\proto\\ArmoniK.Api\\Protos\\V1\\%G"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.7",
    "@angular/common": "~14.2.12",
    "@angular/compiler": "~14.2.12",
    "@angular/core": "~14.2.12",
    "@angular/forms": "~14.2.12",
    "@angular/platform-browser": "~14.2.12",
    "@angular/platform-browser-dynamic": "~14.2.12",
    "@angular/router": "~14.2.12",
    "@cds/core": "^6.2.2",
    "@clr/angular": "^13.10.1",
    "@clr/icons": "^13.0.2",
    "@clr/ui": "^13.8.2",
    "@grpc/grpc-js": "^1.7.3",
    "@ngx-grpc/common": "^3.1.1",
    "@ngx-grpc/core": "^3.1.1",
    "@ngx-grpc/grpc-web-client": "^3.1.1",
    "@ngx-grpc/well-known-types": "^3.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@nrwl/angular": "15.2.4",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.6.0",
    "tslib": "^2.4.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.10",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~14.2.10",
    "@angular/compiler-cli": "~14.2.10",
    "@angular/language-service": "~14.2.10",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@ngx-grpc/protoc-gen-ng": "^3.1.1",
    "@nrwl/cli": "15.2.4",
    "@nrwl/cypress": "15.2.4",
    "@nrwl/eslint-plugin-nx": "15.2.4",
    "@nrwl/linter": "15.2.4",
    "@nrwl/node": "15.2.4",
    "@nrwl/workspace": "15.2.4",
    "@types/google-protobuf": "^3.15.6",
    "@types/jasmine": "~3.5.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.45.1",
    "@typescript-eslint/parser": "~5.45.1",
    "cypress": "^11.2.0",
    "eslint": "~8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "grpc-tools": "^1.12.3",
    "husky": "^7.0.4",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.4.0",
    "nx": "15.2.4",
    "prettier": "^2.8.0",
    "typescript": "~4.6.2"
  },
  "lint-staged": {
    "**": [
      "npm run format:check",
      "npm run lint",
      "npm run test"
    ]
  },
  "packageManager": "yarn@3.3.0"
}
